

transaction inA = "ccccc"
key A = ("priv", "pub")

transaction T = {
	in = inA
	wit = sig A AIAO
	 
	out(sigma:signature) = 
		if versig({A},{sigma}) && hash(sigma)=="ABCD" 
		then hash(sigma)
		else "type error"
		
	val = 1 satoshi
	tlock 97
}

transaction T = {
	in = inA
	wit = sig A AIAO
	 
	out(sigma:bool) = 12 + 12
		
	val = 1 satoshi
	tlock 97
}