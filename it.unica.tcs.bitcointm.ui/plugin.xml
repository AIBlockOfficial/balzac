<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
			contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
			default="true"
			extensions="balzac"
			id="it.unica.tcs.Balzac"
			name="Balzac Editor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
			commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
			<activeWhen>
				<reference
					definitionId="it.unica.tcs.Balzac.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<handler
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
			commandId="it.unica.tcs.Balzac.validate">
		<activeWhen>
			<reference
					definitionId="it.unica.tcs.Balzac.Editor.opened">
			</reference>
		</activeWhen>
		</handler>
		<!-- copy qualified name -->
		<handler
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
			<activeWhen>
				<reference definitionId="it.unica.tcs.Balzac.Editor.opened" />
			</activeWhen>
		</handler>
		<handler
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
			<activeWhen>
				<and>
					<reference definitionId="it.unica.tcs.Balzac.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="it.unica.tcs.Balzac.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="it.unica.tcs.Balzac"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
		<definition id="it.unica.tcs.Balzac.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="it.unica.tcs.Balzac"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="it.unica.tcs.Balzac"
			name="Balzac">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
		</page>
		<page
			category="it.unica.tcs.Balzac"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="it.unica.tcs.Balzac.coloring"
			name="Syntax Coloring">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
		</page>
		<page
			category="it.unica.tcs.Balzac"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="it.unica.tcs.Balzac.templates"
			name="Templates">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
		</page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="it.unica.tcs.Balzac"
			name="Balzac">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="it.unica.tcs.ui.keyword_Balzac"
			label="Balzac"/>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
	<command
			description="Trigger expensive validation"
			id="it.unica.tcs.Balzac.validate"
			name="Validate">
	</command>
	<!-- copy qualified name -->
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			 <command
				 commandId="it.unica.tcs.Balzac.validate"
				 style="push"
				 tooltip="Trigger expensive validation">
			<visibleWhen checkEnabled="false">
				<reference
					definitionId="it.unica.tcs.Balzac.Editor.opened">
				</reference>
			</visibleWhen>
		</command>
		</menuContribution>
		<!-- copy qualified name -->
		<menuContribution locationURI="popup:#TextEditorContext?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="it.unica.tcs.Balzac.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="menu:edit?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="it.unica.tcs.Balzac.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<and>
						<reference definitionId="it.unica.tcs.Balzac.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
					<reference definitionId="it.unica.tcs.Balzac.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
			commandId="org.eclipse.xtext.ui.editor.FindReferences">
			<activeWhen>
				<reference
					definitionId="it.unica.tcs.Balzac.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type
			base-type="org.eclipse.core.runtime.text"
			file-extensions="balzac"
			id="it.unica.tcs.Balzac.contenttype"
			name="Balzac File"
			priority="normal">
		</content-type>
	</extension>
	<!-- adding resource factories -->
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="balzac">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="balzac">
		</resourceServiceProvider>
	</extension>
	<!-- marker definitions for it.unica.tcs.Balzac -->
	<extension
			id="balzac.check.fast"
			name="Balzac Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="balzac.check.normal"
			name="Balzac Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="balzac.check.expensive"
			name="Balzac Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="it.unica.tcs.Balzac"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.validation.ValidatorPreferencePage"
			id="it.unica.tcs.Balzac.validator.preferencePage"
			name="Errors/Warnings">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
		</page>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="balzac"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="it.unica.tcs.Balzac"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="it.unica.tcs.Balzac.compiler.preferencePage"
			name="Compiler">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.propertyPages">
		<page
			category="it.unica.tcs.Balzac"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="it.unica.tcs.Balzac.compiler.propertyPage"
			name="Compiler">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?after=xtext.ui.openDeclaration">
			<command
				commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand"
				id="it.unica.tcs.Balzac.OpenGeneratedCode"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference definitionId="it.unica.tcs.Balzac.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
			commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
			<activeWhen>
				<reference definitionId="it.unica.tcs.Balzac.Editor.opened" />
			</activeWhen>
		</handler>
	</extension>
	<!-- Quick Outline -->
	<extension
		point="org.eclipse.ui.handlers">
		<handler 
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
			commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
			<activeWhen>
				<reference
					definitionId="it.unica.tcs.Balzac.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Open the quick outline."
			id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
			name="Quick Outline">
		</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.open">
			<command commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
				style="push"
				tooltip="Open Quick Outline">
				<visibleWhen checkEnabled="false">
					<reference definitionId="it.unica.tcs.Balzac.Editor.opened"/>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<!-- quickfix marker resolution generator for it.unica.tcs.Balzac -->
	<extension
			point="org.eclipse.ui.ide.markerResolution">
		<markerResolutionGenerator
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="it.unica.tcs.bitcointm.ui.balzac.check.fast">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="it.unica.tcs.bitcointm.ui.balzac.check.normal">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="it.unica.tcs.bitcointm.ui.balzac.check.expensive">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
	</extension>
	<!-- Rename Refactoring -->
	<extension point="org.eclipse.ui.handlers">
		<handler 
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
			commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
			<activeWhen>
				<reference
					definitionId="it.unica.tcs.Balzac.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			<command commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference
						definitionId="it.unica.tcs.Balzac.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="it.unica.tcs.Balzac"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
			id="it.unica.tcs.Balzac.refactoring"
			name="Refactoring">
			<keywordReference id="it.unica.tcs.ui.keyword_Balzac"/>
		</page>
	</extension>
	<extension point="org.eclipse.compare.contentViewers">
		<viewer id="it.unica.tcs.Balzac.compare.contentViewers"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="balzac">
		</viewer>
		<contentTypeBinding
			contentTypeId="it.unica.tcs.Balzac.contenttype"
			contentViewerId="it.unica.tcs.Balzac.compare.contentViewers" />
	</extension>
	<extension point="org.eclipse.compare.contentMergeViewers">
		<viewer id="it.unica.tcs.Balzac.compare.contentMergeViewers"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="balzac" label="Balzac Compare">
		</viewer>
		<contentTypeBinding
			contentTypeId="it.unica.tcs.Balzac.contenttype"
			contentMergeViewerId="it.unica.tcs.Balzac.compare.contentMergeViewers" />
	</extension>
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider id="it.unica.tcs.Balzac.editors.documentProviders"
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.xtext.ui.editor.model.XtextDocumentProvider"
			extensions="balzac">
		</provider>
	</extension>
	<extension point="org.eclipse.team.core.fileTypes">
		<fileTypes
			extension="balzac"
			type="text">
		</fileTypes>
	</extension>
	<!-- code mining -->
	<extension point="org.eclipse.ui.workbench.texteditor.codeMiningProviders">
		<codeMiningProvider
			class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:org.eclipse.jface.text.codemining.ICodeMiningProvider"
			id="it.unica.tcs.Balzac.CodeMiningProvider"
			label="Balzac Codemining">
			<enabledWhen>
				<with variable="editorInput">
					<adapt type="org.eclipse.core.resources.IFile">
						<test
							property="org.eclipse.core.resources.contentTypeId"
							value="it.unica.tcs.Balzac.contenttype">
						</test>
					</adapt>
				</with>
			</enabledWhen>
		</codeMiningProvider>
	</extension>
	<!-- customization -->
    <extension point="org.eclipse.core.runtime.preferences">
        <initializer
              class="it.unica.tcs.ui.preferences.PreferenceInitializer">
        </initializer>
    </extension>
    <extension point="org.eclipse.ui.preferencePages">
        <page category="it.unica.tcs.Balzac"
            class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:it.unica.tcs.ui.preferences.TrustedNodesPreferences"
            id="it.unica.tcs.Balzac.node" name="Trusted Nodes">
            <keywordReference
                id="it.unica.tcs.ui.keyword_Balzac">
            </keywordReference>
        </page>
        <page category="it.unica.tcs.Balzac"
            class="it.unica.tcs.ui.BalzacExecutableExtensionFactory:it.unica.tcs.ui.preferences.KeystoreGenerationPreferences"
            id="it.unica.tcs.Balzac.node" name="Keystore">
            <keywordReference
                id="it.unica.tcs.ui.keyword_Balzac">
            </keywordReference>
            <keywordReference
                id="it.unica.tcs.ui.keyword_Keystore">
            </keywordReference>
        </page>
    </extension>
    <extension
          point="org.eclipse.ui.keywords">
       <keyword
             id="it.unica.tcs.ui.keyword_Keystore"
             label="Keystore">
       </keyword>
    </extension>
</plugin>
